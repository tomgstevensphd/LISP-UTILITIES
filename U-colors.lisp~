;;********************** U-colors.lisp *****************************
;;
;;

;;   (show-my-colors)
;;ddd
(defun show-my-colors ()
  "In U-colors.lisp, shows all my pre-selected LW colors with color names"
  (show-colors *my-main-colors-list1 "This is test text")
  (show-colors *my-main-colors-list2 "This is test text")
  )

;;  (show-all-colors)
;;ddd
(defun show-all-colors ()
  "In U-colors.lisp, shows all LW colors with color names"
  (show-colors *lw-colors-list1  "This is test text")
  (show-colors *lw-colors-list2  "This is test text")
  (show-colors *lw-colors-list3  "This is test text")
  (show-colors *lw-colors-list4  "This is test text")
  (show-colors *lw-colors-list5  "This is test text"))

;;
;;SAMPLE-COLORS
;;
;;ddd
(defun show-colors (color-list test-text)
  "In U-colors.lisp, shows color name, test-text, and a rectangle of each color from color-list"
  ;;use global vars here
  (setf *temp-color-list color-list
        *temp-test-text test-text)
  (let
      ((instance (make-instance 'Color-Test-Interface))
       (cur-rectangle)
       (print-string)
       (x-test 10)
       (x-rect 30)
       (y-current 0)
       (i 0)
       )
    ;;display the instance
    (capi:display instance)
    ;;make color list of name, sample text, colored rectangle
    (dolist (color color-list)
      (incf i)
      (if  (or (= i 37)(= i 72))
          (setf y-current 0))

      ;;make columns
      (cond
       ((< i 36)
        (setf  x-text 10
               x-rect 220
               y-current (+ y-current 20)))
       ((and (>= i 36) (< i 72))
        (setf  x-text 350
               x-rect 550
               y-current (+ y-current 20)))
       ((>= i 72)
        (setf  x-text 700
               x-rect 950
               y-current (+ y-current 20))))       

      (with-slots (output-pane-1) instance
        (gp:draw-string output-pane-1 (format nil "~A" color) x-text y-current :foreground :black)
        (gp:draw-string output-pane-1 test-text (+ x-text 120) y-current :foreground color)
        (gp:draw-rectangle output-pane-1 x-rect (- y-current 15) 80 15 :filled t :foreground color)
        ))
    ))
        
 (capi:define-interface Color-Test-Interface ()
   "In U-colors.lisp, used for showing samples of  LW colors by color names"
    ()
    (:panes
     (output-pane-1
      capi:output-pane)
     (title-pane-1
      capi:title-pane
      :text "Color Test Window"))
    (:layouts
     (column-layout-1
      capi:column-layout
      '(output-pane-1 title-pane-1)))
    (:default-initargs
     :best-height 800
     :best-width 1100
     :scroll-vertical-page-size 800
     :vertical-scroll t
    :scroll-if-not-visible-p t
     :layout 'column-layout-1
     :iconify-callback 'redisplay-callback
     :title "Color-Test-Interface"))      

(defun redisplay-callback (data interface)
  (sample-colors *temp-color-list *temp-test-text))
      

